        enum Tokens{FLOAT_CONSTANT=0x100,
	        INT_CONSTANT,
	        IDENTIFIER,
	        VOID,
	        INT,
	        FLOAT,
	        IF,
	        ELSE,
	        WHILE,
	        FOR,
	        RETURN,
	        EQ_OP,
	        NE_OP,
	        GE_OP,
	        LE_OP,
	        AND_OP,
	        OR_OP,
	        INC_OP,
	        OTHERS,
	        STRING_LITERAL};
